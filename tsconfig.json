{
    "compilerOptions": {
        "strict": true,
        "noImplicitAny": true,
        "esModuleInterop": true,
        "strictNullChecks": true,
        "target": "ES5",
        "moduleResolution": "Node10",
        "module": "esnext",
        "declaration": true,
        "isolatedModules": true,
        "noEmit": true,
        "outDir": "dist",
        "jsx": "react"
    },
    "include": ["src"],
    "exclude": ["node_modules"]
}